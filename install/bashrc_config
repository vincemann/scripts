# Aliases
# needs to be done so aliases also work with sudo
alias sudo='sudo '
alias s='sudo '
# text editor struggle
alias e='sudo gedit'
# make eXecuteable
alias x='sudo chmod +x'
alias ?='helpCommands'
# create
alias c='gedit'
alias explorer='startExplorer &'
# find aliases
alias fdp='source findProjectDir'
alias fd='source findDir'
alias fdoc='findDoc'
alias fdocd='source findDocDir'
alias f='findGlobal'
alias fed='sudo $(whereis findAndEdit | cut -d " " -f2-)'



# VINCEMANN ALIASES

# Aliases

# needs to be done so aliases also work with sudo
alias sudo='sudo '
alias s='sudo '

alias fpd='source findProjectDir'
alias fd='source findDir'
alias fdoc='findDoc'
alias fdocd='source findDocDir'
alias f='findGlobal'
alias fed='sudo $(whereis findAndEdit | cut -d " " -f2-)'


# END VINCEMANN ALIASES

